package com.ready.rain.entity.mib;

public class Bullet {

	public double angle = 0;
	public Gun gun;
	public static int x = 0;
	public static int y = 0;
	
	public Bullet (double angle, Gun gun,int x,int y){
		this.angle = angle;
		this.gun = gun;
		this.x = x;
		this.y = y;
	}
	public static int getBulletx(){
		return x;
	}
	
	public static int getBullety(){
		return y;
	}
}
