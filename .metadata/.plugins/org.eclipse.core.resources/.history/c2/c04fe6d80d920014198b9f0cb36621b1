package com.ready.rain.entity.mib;

import java.awt.image.BufferedImage;

public class Gun {

		public BufferedImage gun;
		public BufferedImage gun2;
		public String type;
		
		public Gun (
				BufferedImage gun,
				BufferedImage gun2,
				String type
				) {

			this.gun = gun;
			this.gun2 = gun2;
			this.type = type;
			}
}
